# Production Environment Configuration
# Copy this file to .env.production.local and update with your production values

# Database - Use PostgreSQL or MySQL for production
# Example PostgreSQL: postgresql://username:password@localhost:5432/database_name
# Example MySQL: mysql://username:password@localhost:3306/database_name
DATABASE_URL="file:./prod.db"

# Next Auth - CRITICAL: Change these for production
NEXTAUTH_URL="https://phuong-landing-page.com"
NEXTAUTH_SECRET="your-super-secure-secret-key-at-least-32-characters-long"

# Admin Credentials - CRITICAL: Change these for production
ADMIN_USERNAME="admin"
ADMIN_PASSWORD="admin123^^"

# Optional: Additional production settings
NODE_ENV="production"